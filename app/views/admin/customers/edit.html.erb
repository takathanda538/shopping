<%= form_with(model: @customer, url: admin_customer_path, local: true ,method: :patch) do |f|%>
  <%= f.text_field :last_name %>
  <%= f.text_field :first_name %>
  <%= f.text_field :prefectur %>
  <%= f.text_field :address %>
  <%= f.text_field :phone_number %>
  <%= f.text_field :is_deleted  %>
  <%= f.submit "更新する" %>
<% end %>
